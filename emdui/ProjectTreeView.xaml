    <Grid>
        <TreeView Grid.Row="0"
                  x:Name="treeView"
                  Background="{x:Null}"
                  BorderThickness="0"
                  MouseRightButtonDown="treeView_MouseRightButtonDown"
                  PreviewMouseDoubleClick="treeView_PreviewMouseDoubleClick"
                  SelectedItemChanged="treeView_SelectedItemChanged">
            <TreeView.ContextMenu>
                <ContextMenu>
                    <MenuItem Header="Import..." Click="importMenuItem_Click" />
                    <MenuItem Header="Export..." Click="exportMenuItem_Click" />
                </ContextMenu>
            </TreeView.ContextMenu>
        </TreeView>
    </Grid>
